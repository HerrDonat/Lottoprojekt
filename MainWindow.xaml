<Window x:Class="Lottoprojekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lottoprojekt"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#04c582" ResizeMode="NoResize">
    <Grid>
        <TextBox Name="numberOne" HorizontalAlignment="Left" Height="39" Margin="21,43,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
        <TextBox Name="numberTwo" HorizontalAlignment="Left" Height="39" Margin="107,43,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
        <TextBox Name="numberThree" HorizontalAlignment="Left" Height="39" Margin="184,43,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
        <TextBox Name="numberFour" HorizontalAlignment="Left" Height="39" Margin="265,43,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
        <TextBox Name="numberFive" HorizontalAlignment="Left" Height="39" Margin="344,43,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="56"/>
        <TextBox Name="numberSix" HorizontalAlignment="Left" Height="39" Margin="424,43,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
        <TextBox Name="numberSuper" HorizontalAlignment="Left" Height="39" Margin="532,43,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="57"/>
        <Button x:Name="history" Content="Meine letzten Ziehungen" HorizontalAlignment="Left" Height="55" Margin="648,335,0,0" VerticalAlignment="Top" Width="152" Click="history_Click"/>
        <Label Content="Zahl1" HorizontalAlignment="Left" Height="29" Margin="21,9,0,0" VerticalAlignment="Top" Width="57"/>
        <Label Content="Zahl2" HorizontalAlignment="Left" Height="29" Margin="107,10,0,0" VerticalAlignment="Top" Width="57"/>
        <Label Content="Zahl3" HorizontalAlignment="Left" Height="29" Margin="184,10,0,0" VerticalAlignment="Top" Width="57"/>
        <Label Content="Zahl4" HorizontalAlignment="Left" Height="29" Margin="265,10,0,0" VerticalAlignment="Top" Width="57"/>
        <Label Content="Zahl5" HorizontalAlignment="Left" Height="29" Margin="344,10,0,0" VerticalAlignment="Top" Width="57"/>
        <Label Content="Zahl6" HorizontalAlignment="Left" Height="29" Margin="424,10,0,0" VerticalAlignment="Top" Width="56"/>
        <Label Content="Superzahl" HorizontalAlignment="Left" Height="29" Margin="532,7,0,0" VerticalAlignment="Top" Width="72"/>
        <Button x:Name="ApplyNumbers" Content="Tipps bestätigen" HorizontalAlignment="Left" Height="36" Margin="663,44,0,0" VerticalAlignment="Top" Width="100" Click="UploadTipp"/>
        <TextBox x:Name="UserEID" HorizontalAlignment="Left" Height="41" Margin="0,393,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="126" IsEnabled="False"/>
        <Image x:Name="LottoImage" HorizontalAlignment="Center" Height="222" VerticalAlignment="Top" Width="354" Source="index.png" Margin="0,212,0,0"/>
        <DataGrid x:Name="dataGrid1" AutoGenerateColumns="True" HorizontalAlignment="Left" Height="209" Margin="605,114,0,0" VerticalAlignment="Top" Width="195"/>
        <Label x:Name="MitarbeiterText" Content="Sie sind als Mitarbeiter angemeldet." HorizontalAlignment="Center" Height="48" Margin="0,164,0,0" VerticalAlignment="Top" Width="252" FontWeight="Bold" Visibility="Hidden"/>
    </Grid>
</Window>
